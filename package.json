{
  "name": "client",
  "version": "1.0.0",
  "description": "Repository containing the client's package.json and other important files.\r All updates and changes should be made through this repository.",
  "main": "./dist/index.html",
  "scripts": {
    "start": "npm run dev",
    "dev": "cross-env NODE_ENV=development node --max_old_space_size=4048 node_modules/webpack-dev-server/bin/webpack-dev-server.js --progress --config webpack.config.js",
    "dev:eval": "cross-env NODE_ENV=development DEVTOOL=eval webpack-dev-server --progress --open",
    "dev:share": "npm run build && superstatic dist --port 8080 --host 0.0.0.0 -c superstatic.json",
    "dev:unminified": "cross-env NODE_ENV=development APP_CONF=unminified webpack-dev-server --progress --open",
    "dev:eval-unminified": "cross-env NODE_ENV=development DEVTOOL=eval APP_CONF=unminified webpack-dev-server --progress --open",
    "build": "cross-env NODE_ENV=development webpack --progress",
    "build:prod": "cross-env NODE_ENV=production webpack --progress",
    "clean": "rimraf dist",
    "clean:modules": "rimraf node_modules",
    "clean:empty-dirs": "delete-empty ./app",
    "clean:less-types": "rimraf ./app/**/*.less.d.ts",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage --colors",
    "test:debug": "node --inspect-brk ./node_modules/jest/bin/jest.js --runInBand --no-cache",
    "tslint": "tslint --fix -c tslint.json",
    "tslint:all": "tslint --fix -c tslint.json 'app/**/*.{ts,tsx}'",
    "tslint:check": "tslint-config-prettier-check ./tslint.json",
    "prettier": "prettier --write",
    "prettier:all": "prettier --write \"app/**/*.{ts,tsx}\"",
    "init:all": "npm run clean:modules & npm run init:modules && npm run init:hooks",
    "init:hooks": "ncp ./hooks/ ./.git/hooks",
    "init:modules": "winrar.exe x -inul -o+ node_modules_backup *.* .",
    "docker": "npm run docker:build && npm run docker:save",
    "docker:build": "npm run clean && npm run build && docker-machine env dev > temp.bat && git log -n1 --format=\"SET DOCKER_COMMIT_ID=%H\" -n 1 >> temp.bat && temp.bat && docker build -t bluetorch/bluetorch-client --build-arg commitId=%DOCKER_COMMIT_ID% ./ && del temp.bat",
    "docker:save": "docker-machine env dev > temp.bat && temp.bat && del temp.bat && docker save bluetorch/bluetorch-client -o bluetorch-client.tar.gz"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "post-commit": "git update-index -g"
    }
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "npm run tslint",
      "npm run prettier",
      "git add"
    ]
  },
